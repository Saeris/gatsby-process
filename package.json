{
  "name": "@saeris/gatsby-process",
  "private": true,
  "author": "Drake Costa <drake@saeris.io> (https://github.com/Saeris/)",
  "description": "",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "typescript",
    "styled-components",
    "reakit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Saeris/gatsby-process"
  },
  "bugs": {
    "url": "https://github.com/Saeris/gatsby-process/issues"
  },
  "scripts": {
    "build": "run-s build:**",
    "build:app": "gatsby build",
    "build:lambda": "cross-env NODE_ENV=`production` LAMBDA=true netlify-lambda build src/lambda --config ./webpack.functions.js",
    "dev": "yarn start",
    "start": "run-p -l start:**",
    "start:app": "cross-env CHOKIDAR_USEPOLLING=true gatsby develop",
    "start:lambda": "cross-env LAMBDA=true npx --node-arg=--inspect netlify-lambda serve src/lambda --config ./webpack.functions.js",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.5.8",
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "babel-preset-gatsby": "^0.3.1",
    "framer-motion": "1.10.3",
    "gatsby": "^2.20.10",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-catch-links": "^2.2.1",
