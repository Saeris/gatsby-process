    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "esm": "^3.2.25",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "netlify-lambda": "1.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "^2.0.2",
    "remark-breaks": "^1.0.5",
    "remark-heading-id": "^1.0.0",
    "remark-unwrap-images": "^2.0.0",
    "webpack-node-externals": "^1.6.0"
  },
  "resolutions": {
    "graphql": "^14.6.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.svg": [
      "svgo --pretty --indent=2 --multipass",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
