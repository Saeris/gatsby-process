    "typeface-fira-code": "^1.1.4",
    "typeface-montserrat": "0.0.75",
    "typeface-varela-round": "^0.0.71",
    "typeface-work-sans": "^0.0.72",
    "typescript": "^3.8.3",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
